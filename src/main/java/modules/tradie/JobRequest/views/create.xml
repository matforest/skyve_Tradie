<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="create"
	title="Job Request"
	xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
	<form responsiveWidth="12" percentageWidth="60" border="true">
		<column responsiveWidth="4" />
		<column />
		<row>
			<item>
				<default binding="customerName" />
			</item>
		</row>
		<row>
			<item>
				<default binding="phone" />
			</item>
		</row>
		<row>
			<item>
				<default binding="address" />
			</item>
		</row>
		<row>
			<item>
				<default binding="directions" />
			</item>
		</row>
		<row>
			<item>
				<default binding="description" />
			</item>
		</row>
		<row>
			<item>
				<default binding="requestType" />
			</item>
		</row>
		<row>
			<item>
				<radio binding="urgency">
					<onFocusHandlers />
					<onBlurHandlers />
					<onChangedHandlers />
				</radio>
			</item>
		</row>
	</form>
	<actions>
		<defaults />
	</actions>
	<newParameters />
</view>
